<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<fieldset label="$fieldset_label">
<notebook>

 <page label="définition">
  <form>
   <hbox>
    <vbox width="34%">
     <elem field="name" />
    </vbox>
    <vbox width="34%">
     <elem field="code" />
    </vbox>
    <vbox>
     <elem field="image" type="file" />
    </vbox>
   </hbox>
   <launcher/>
  </form>
 </page>

 <page label="opérations" built="$is_loaded">
  <hbox>
   <vbox width="30%">
    <box label="actions">
     <link icon="details" />
    </box>
   </vbox>
   <vbox><deck /></vbox>
  </hbox> 
 </page>

</notebook>
</fieldset>

