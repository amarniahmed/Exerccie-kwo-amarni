<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<fieldset label="$fieldset_label">

<notebook>

<page label="définition">
 <form>
  <hbox>
   <vbox width="40%">
    <elem value="$content" rows="4" label="contenu" />
    <elem value="$status" type="yesno" label="traité ?" />
   </vbox>
   <vbox>
    <grid value="$details" label="details" />
   </vbox>
  </hbox>
  <launcher/>
 </form>
</page>

<page label="opérations" built="$is_loaded">
 <hbox>
  <vbox width="30%">
   <box label="actions">
    <link icon="details" />
    <link icon="note" />
   </box>
  </vbox>
  <vbox><deck /></vbox>
 </hbox> 
</page>

</notebook>

</fieldset>

