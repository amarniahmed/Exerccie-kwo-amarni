<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<fieldset label="$fieldset_label">
 <notebook>
  <page label="définition">
   <form>
    <hbox>
     <vbox>
      <elem field="name" spellcheck="false" />
      <elem field="model_id" options="Property::models()" label="modèle associé" />
      <elem field="flags" cols="1" />
     </vbox>
     <vbox>
      <elem field="description" rows="4" />
     </vbox>
    </hbox>
    <launcher/>
   </form>
  </page>
  <page label="opérations" built="$is_loaded">
   <hbox>
    <vbox width="30%">
     <box label="actions">
      <link icon="details" />
      <link icon="note" />
     </box>
    </vbox>
    <vbox><deck /></vbox>
   </hbox> 
  </page>
 </notebook>
</fieldset>
