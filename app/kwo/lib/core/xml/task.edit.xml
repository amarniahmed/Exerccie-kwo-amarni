<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<fieldset label="$fieldset_label">

<notebook>
<page label="définition">
 <form>
  <hbox>
   <vbox>
    <elem field="name" />
    <elem field="code" />
   </vbox>
   <vbox>
     <elem field="method" />
     <elem field="inputs_user" rows="3" label="Inputs de l'utilisateur" />
     <elem field="inputs" rows="3" />
     <elem field="outputs" rows="3" />
     <elem field="params" rows="3" />
   </vbox>
  </hbox>
  <launcher/>
 </form>
</page>
<page label="opérations" built="$is_loaded">
 <hbox>
  <vbox width="150px">
   <box label="actions">
    <link icon="details" />
   </box>
  </vbox>
  <vbox><deck /></vbox>
 </hbox> 
</page>
</notebook>

</fieldset>

