<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<fieldset label="$fieldset_label">
<notebook>

 <page label="définition">
  <form>
   <hbox>
    <vbox width="34%">
     <elem value="$name" label="nom" />
    </vbox>
    <vbox width="34%">
     <elem value="$description" rows="3" label="description" />
    </vbox>
    <vbox>
     <elem value="$status" type="yesno" label="valide" />
    </vbox>
   </hbox>
   <launcher/>
  </form>
 </page>

 <page label="opérations" built="$is_loaded">
  <hbox>
   <vbox width="30%">
    <box label="actions">
     <link icon="details" />
     <link icon="note" />
     <link icon="tag" built="false" />
    </box>
   </vbox>
   <vbox><deck /></vbox>
  </hbox> 
 </page>

</notebook>
</fieldset>

