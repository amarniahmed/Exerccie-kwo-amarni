<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<kwo-interface>
<fieldset label="$fieldset_label">

<form>
<notebook>

<page label="définition">
 <hbox>
  <vbox width="34%">
   <elem value="$email" label="email" required="true" />
  </vbox>
  <vbox width="34%">
   <elem value="$last_name" label="nom" />
   <elem value="$first_name" label="prénom" />
  </vbox>
  <vbox>
   <elem value="$base_id" options="Model::asHash('Base')" label="base" />
  </vbox>
 </hbox>
 <launcher/>
</page>

<page label="opérations" built="$is_loaded">
 <hbox>
  <vbox width="30%">
   <box label="actions">
    <link icon="details" />
   </box>
  </vbox>
  <vbox>
   <deck/>
  </vbox>
 </hbox> 
</page>

</notebook>
</form>

</fieldset>
</kwo-interface>